{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "web-app",
        "image": "072061963333.dkr.ecr.us-east-1.amazonaws.com/justin-resp:latest",
        "environment": [
          {
            "name": "Container",
            "value": "nodejs"
          }
        ],
        "essential": true,
        "memory": 128,
        "portMappings": [
            {
              "hostPort": 80,
              "containerPort": 80
            }
          ]
      }
    ]
  }